{
  "source": {
    "include": ["." ],
    "exclude": ["./config", "./node_modules", "./public"]
  },
  "opts": {
    "recurse": true,
    "destination": "./docs"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "cerulean",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
