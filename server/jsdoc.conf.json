{
  "source": {
    "include": ["./src", "README.md"],
    "exclude": ["./node_modules"]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "recurse": true,
    "destination": "./docs",
    "encoding": "utf8",
    "readme": "./README.md",
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
